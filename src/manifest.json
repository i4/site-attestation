{

    "description": "Enables to perform remote attesation on AMD SEV-SNP secured Boundary Node virtual machine of the IC",
    "manifest_version": 2,
    "name": "check-vm",
    "version": "1.0",
    "homepage_url": "https://gitlab.com/ruediger.kapitza/twebload",
    "icons": {
      "48": " ravm.svg"
    },  
  
    "permissions": [
      "webRequest", 
      "webRequestBlocking",
      "*://transparent-vm.net/*",
      "*://*/*"
    ],
  
    "background": {
      "scripts": ["background.js"]
    },  
  
    "browser_specific_settings": {
      "gecko": {
        "strict_min_version": "57.0a1"
      }   
    }
  
  }
  

