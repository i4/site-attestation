{

    "manifest_version": 2,
    "name": "check-vm",
    "homepage_url": "https://gitlab.com/ruediger.kapitza/twebload",
    "icons": {
      "48": "icons/ravm.svg"
    },  
  
    "permissions": [
      "webRequest", 
      "webRequestBlocking",
      "*://transparent-vm.net/*",
      "*://*/*"
    ],
  
    "background": {
      "scripts": [
        "background/index.js"
      ]
    },
  
    "browser_specific_settings": {
      "gecko": {
        "strict_min_version": "57.0a1"
      }   
    }
  
  }
  

